{
    "version": 2,
    "builds": [
      {
        "src": "videoclub/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "servidor/**",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "servidor/$1"
      },
      {
        "src": "/(.*)",
        "dest": "videoclub/$1"
      }
    ]
  }
  